<!DOCTYPE html>
<html>
  <head>
    <title>Employee Details</title>
  </head>
  <body>
    <h1>Employee Details</h1>
    <h2>Name: <%= employee.name %></h2>
    <h2>Contact Details:</h2>
    <ul>
      <% contactDetails.forEach((contact) => { %>
      <li><strong><%= contact.type %>:</strong> <%= contact.value %></li>
      <% }) %>
    </ul>
  </body>
</html>
